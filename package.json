{
  "name": "ng2-gauge",
  "version": "1.0.3",
  "description": "Gauge component for Angular",
  "main": "index.js",
  "scripts": {
    "transpile": "ngc",
    "package": "rollup -c",
    "minify": "uglifyjs dist/bundles/ng2-gauge.umd.js --screw-ie8 --compress --mangle --comments --output dist/bundles/ng2-gauge.umd.min.js",
    "build": "npm run transpile && npm run package && npm run minify"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/hawkgs/ng2-gauge.git"
  },
  "keywords": [
    "angular",
    "angular2",
    "angular 2",
    "angular4",
    "gauge",
    "ng gauge",
    "angular gauge"
  ],
  "author": "hawkgs (Georgi Serev)",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/hawkgs/ng2-gauge/issues"
  },
  "homepage": "https://github.com/hawkgs/ng2-gauge#readme",
  "devDependencies": {
    "@angular/common": "^4.3.1",
    "@angular/compiler": "^4.3.1",
    "@angular/compiler-cli": "^4.3.1",
    "@angular/core": "^4.3.1",
    "rollup": "^0.45.2",
    "rxjs": "^5.4.2",
    "typescript": "^2.4.2",
    "uglify-js": "^3.0.25",
    "zone.js": "^0.8.14"
  }
}
